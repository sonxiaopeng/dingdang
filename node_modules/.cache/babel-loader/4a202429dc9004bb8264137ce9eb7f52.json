A,CAAA,OAAA,CAAA,IAAA,CAAA,GAAA;AACA,SAJA,MAIA;AACA,UAAA,KAAA,CAAA,MAAA,CAAA;AACA,YAAA,OAAA,0CAAA,GAAA,CAAA,IAAA,CAAA,OAAA,CADA;AAEA,YAAA,QAAA,EAAA,QAFA;AAGA,YAAA,QAAA,EAAA;AAHA,WAAA;AAKA;AACA,OAhBA,EAiBA,KAjBA,CAiBA,UAAA,MAAA;AAAA,eAAA,OAAA,CAAA,GAAA,CAAA,MAAA,CAAA;AAAA,OAjBA;AAkBA;AAtCA;AARA,CAAA","sourcesContent":["<template>\r\n\t<div id=\"login\">\r\n\t\t<van-nav-bar\r\n\t\t\ttitle=\"个人中心\"\r\n\t\t\tleft-arrow\r\n\t\t\t@click-left=\"onClickLeft\"\r\n\t\t/>\r\n        <van-field v-model=\"username\" placeholder=\"请输入手机号\" />\r\n        <van-field v-model=\"password\" placeholder=\"请输入密码\" />\r\n        <van-field v-model=\"repassword\" placeholder=\"请确认密码\" />\r\n        <van-button @click=\"login\" class=\"login-btn\" color=\"#00c792\" round type=\"primary\" size=\"large\">登录</van-button>\r\n        <van-button color=\"#00c792\" round plain type=\"primary\" size=\"large\">注册</van-button>\r\n\t</div>\r\n</template>\r\n\r\n<script>\r\nimport { Toast } from 'vant';\r\nexport default {\r\n\tdata() {\r\n\t\treturn {\r\n            username: '',\r\n            password: '',\r\n            repassword: '',\r\n        };\r\n    },\r\n    methods: {\r\n        onClickLeft(){\r\n\r\n        },\r\n        login(){\r\n                var usernameReg = /^1[0-9]{12}$/;\r\n                var passwordReg = /^[a-zA-Z](?![0-9]+$)(?![a-zA-Z]+$)[a-zA-Z0-9]{5,19}$/;\r\n                if(!usernameReg.test(this.username)){\r\n                    \r\n                    Toast('手机号不正确');\r\n                    return false;\r\n                }\r\n                if(!passwordReg.test(this.password)){\r\n                    Toast('密码格式不正确');\r\n                    return false;\r\n                }\r\n                if(repassword != password){\r\n                    Toast('不正确');\r\n                    return false;\r\n                }\r\n                this.axios.post('/login', {\r\n                    username: this.username,\r\n                    password: this.password\r\n                })\r\n                .then(res=>{\r\n                    if(res.data.code == 0){\r\n                        // this.$store.state.user = res.data.data\r\n                        this.$store.commit('updateUserInfo', res.data.data)\r\n                        this.$router.push('/')\r\n                    }else{\r\n                        this.$toast({\r\n                            message: `登录失败：${res.data.message}`,\r\n                            position: 'middle',\r\n                            duration: 3000\r\n                        });\r\n                    }\r\n                })\r\n                .catch(reason=>console.log(reason))\r\n            }\r\n    }\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n#login .van-field {\r\n    padding: 20px 20px;\r\n    font-size: 16px;\r\n}\r\n\r\n#login .login-btn {\r\n    margin: 20px 0;\r\n}\r\n\r\n</style>\r\n"],"sourceRoot":"src/views"}]}