lor: #999;\r\n  width: 100%;\r\n}\r\n.van-tree-select__nav-item {\r\n  padding: 16px 12px;\r\n}\r\n#address >>> .van-sidebar-item__text:visited {\r\n  font-weight: 400;\r\n  color: #00c792;\r\n}\r\n#address >>> .van-sidebar-item--select .van-sidebar-item__text{\r\n  color: #00c792;\r\n}\r\n</style>\r\n<script>\r\nexport default {\r\n  data() {\r\n    return {\r\n      item: {\r\n          // 导航名称\r\n          text: \"热门地区\",\r\n          // 导航节点额外类名\r\n          // className: 'my-class',\r\n          // 该导航下所有的可选项\r\n          children: [\r\n            {\r\n              // 名称\r\n              text: \"同城(定位失败)\",\r\n              // id，作为匹配选中状态的标识符\r\n              id: 0,\r\n            },\r\n            {\r\n              text: \"全国\",\r\n            },\r\n            {\r\n              text: \"北京\",\r\n              id: 1,\r\n            },\r\n            {\r\n              text: \"上海\",\r\n              id: 3,\r\n            },\r\n            {\r\n              text: \"广州\",\r\n              id: 213,\r\n            },\r\n            {\r\n              text: \"深圳\",\r\n              id: 214,\r\n            },\r\n          ],\r\n        },\r\n      items: [],\r\n      activeId: 0,\r\n      activeIndex: 0,\r\n      height: 600,\r\n    };\r\n  },\r\n  methods: {\r\n    onClickLeft() {\r\n      this.$router.push(\"/hospital\");\r\n    },\r\n    initactiveId() {\r\n      this.activeId = 1;\r\n    },\r\n    hospital(data){\r\n      console.log(data)\r\n      this.$router.push(`/hospital?city_id=${data.id}`)\r\n    }\r\n  },\r\n  mounted() {\r\n    this.axios.get(\"/addressbar\").then((result) => {\r\n      // this.items = result.data.data;\r\n      console.log(this.items);\r\n      let total = [];\r\n      let provinces = result.data.data[1]\r\n      let citys = result.data.data[0]\r\n      provinces.forEach((province) => {\r\n         let tempCity = citys.filter((city) => {\r\n          return province.province_id == city.province_id;\r\n          \r\n        });\r\n        if(tempCity.length == 1){return}\r\n        tempCity = tempCity.map((c) => {\r\n          return { text: c.cityname, id: c.city_id };\r\n        });\r\n\r\n        total.push({ text: province.province_name, children: tempCity });\r\n      });\r\n      total.unshift(this.item)\r\n      this.items = total\r\n      console.log(this.items)\r\n    });\r\n  },\r\n};\r\n</script>"],"sourceRoot":"src/views"}]}