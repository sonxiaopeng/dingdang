IAAA,IAAA,CAAA,EAAA;AACA,UAAA,KAAA,CAAA,OAAA,CAAA;;AACA,UAAA,KAAA,CAAA,OAAA,CAAA,IAAA,CAAA,QAAA;AAEA,SAJA,MAIA;AACA,UAAA,KAAA,CAAA,WAAA,CAAA;AACA;AACA,OAZA,EAaA,KAbA,CAaA,UAAA,MAAA;AAAA,eAAA,OAAA,CAAA,GAAA,CAAA,MAAA,CAAA;AAAA,OAbA;AAcA;AArCA;AARA,CAAA","sourcesContent":["<template>\r\n\t<div id=\"register\">\r\n\t\t<van-nav-bar\r\n\t\t\ttitle=\"个人中心\"\r\n\t\t\tleft-arrow\r\n\t\t\t@click-left=\"onClickLeft\"\r\n\t\t/>\r\n        <van-field v-model=\"username\" placeholder=\"请输入手机号\" />\r\n        <van-field v-model=\"password\" placeholder=\"请输入密码\" />\r\n        <van-field v-model=\"repassword\" placeholder=\"请确认密码\" />\r\n        <van-button @click=\"register\" class=\"register-btn\" color=\"#00c792\" round type=\"primary\" size=\"large\">注册</van-button>\r\n        <van-button @click=\"gotoLogin\" color=\"#00c792\" round plain type=\"primary\" size=\"large\">登录</van-button>\r\n\t</div>\r\n</template>\r\n\r\n<script>\r\nimport { Toast } from 'vant';\r\nexport default {\r\n\tdata() {\r\n\t\treturn {\r\n            username: '',\r\n            password: '',\r\n            repassword: '',\r\n        };\r\n    },\r\n    methods: {\r\n        onClickLeft(){\r\n            this.$router.back(-1)\r\n        },\r\n        gotoLogin(){\r\n            this.$router.push('/login')\r\n        },\r\n        register(){\r\n                var usernameReg = /^1[0-9]{12}$/;\r\n                var passwordReg = /^[a-zA-Z](?![0-9]+$)(?![a-zA-Z]+$)[a-zA-Z0-9]{5,19}$/;\r\n                if(!usernameReg.test(this.username)){\r\n                    \r\n                    Toast('手机号不正确');\r\n                    return false;\r\n                }\r\n                if(!passwordReg.test(this.password)){\r\n                    Toast('密码格式不正确');\r\n                    return false;\r\n                }\r\n                if(repassword != password){\r\n                    Toast('输入的密码不一致');\r\n                    return false;\r\n                }\r\n                this.axios.post('/register', {\r\n                    username: this.username,\r\n                    password: this.password\r\n                })\r\n                .then(res=>{\r\n                    if(res.data.code == 0){\r\n                        Toast('注册成功！');\r\n                        this.$router.push('/login')\r\n                        \r\n                    }else{\r\n                        Toast('注册失败！请重试！');\r\n                    }\r\n                })\r\n                .catch(reason=>console.log(reason))\r\n            }\r\n    }\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n#register .van-field {\r\n    padding: 20px 20px;\r\n    font-size: 16px;\r\n}\r\n\r\n#register .register-btn {\r\n    margin: 20px 0;\r\n}\r\n\r\n</style>\r\n"],"sourceRoot":"src/views"}]}